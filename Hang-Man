Word = input("Enter A Word: ");
Try = input("Enter A Amount Of Trys To Get The Anser: ");
Player = input("Enter Your Player's Name: ");
Trys =  (Try)
Code = (Word)
guess = input("What Word Do You Want To Guess? ") 
while (guess in Code) == False:
  print("Your Input is: " + guess)
  print("Wrong! Guess Again~")
  guess = input("Try Again: What word do you guess? ") 
  continue
